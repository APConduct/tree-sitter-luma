{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "luma",
      "camelcase": "Luma",
      "title": "Luma",
      "scope": "source.luma",
      "file-types": ["lx"],
      "injection-regex": "^luma$",
      "class-name": "TreeSitterLuma",
      "highlights": ["queries/highlights.scm"]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Luma grammar for tree-sitter",
    "authors": [
      {
        "name": "Perry",
        "email": "aidanpj18@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-luma"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
